public class ArrayQueue {
   private Object[] data;
   private int head;
   private int tail;
   private int quantity;


   public ArrayQueue() {
       this(10);
   }


   public ArrayQueue(int lenght) {
       Object[] data = new Object[lenght];
       head = 0;
       tail = -1;
       quantity = 0;
   }
   public void enqueue(Object data) {
       if (isFull()) {
           System.err.println("Queue is full!");
       } else {
           tail++;
           this.data[tail] = data;
       }
   }
   public Object dequeue() {
       Object aux = null;
       if (isEmpty()) {
           System.err.println("Queue is empty!");
       } else {
           head++;
           aux = data[head];
       }
       return aux;
   }
   public Object front() {
       Object aux = null;
       if(isEmpty()) {
           System.err.println("Queue is empty");
       } else {
           aux = data[head];
       }
       return aux;
   }
   boolean isEmpty() {
       return (quantity == 0);
   }


   boolean isFull () {
       return (quantity == data.length);
   }
   public String print() {
       if (isEmpty()) {
           return "";
       }
       String result = "[";
       for (int i = 0; i < this.quantity; i++) {
           result += this.data[(this.head + i) % this.data.length];
           if (i < this.quantity - 1) {
               result += ", ";
           }
       }
       result += "]";
       return result;
   }
}



